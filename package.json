{
  "name": "@uniformdev/gdogs-site-next",
  "version": "4.0.0-uniformdev",
  "publishConfig": {
    "registry": "https://uniform.dev/no-matter-what-url/"
  },
  "private": true,
  "license": "SEE LICENSE IN PROVIDED DISTRIBUTION PACKAGE",
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  },
  "scripts": {
    "test": "jest --forceExit",
    "test:update": "jest --updateSnapshot",
    "postinstall": "ncp lib node_modules",
    "build": "next build",
    "build:functions": "yarn copy-forms-util && netlify-lambda build ./functions_src",
    "build:storybook": "build-storybook",
    "dev": "node ./server.js",
    "dev:netlify": "netlify dev --functions=functions --command='node ./server.js'",
    "dev:app": "npm-run-all --serial export dev:netlify",
    "start": "npm run build && npm run start:connected",
    "start:connected": "cross-env NODE_ENV=production node ./server.js",
    "export": "npm-run-all --serial export:build export:final",
    "export:build": "npm-run-all --parallel generate-all build",
    "export:final": "npm-run-all --serial export:site generate-404-function-template build:functions",
    "export:site": "next export --threads 80",
    "copy-forms-util": "tsc utils/formUtils.ts --outDir functions_src",
    "generate-all": "npm-run-all --parallel generate-forms retrieve-static-files-and-update generate-sitemap generate-redirects",
    "generate-forms": "tsc generate-form-data.ts && node generate-form-data.js",
    "generate-redirects": "node generate-redirects.js",
    "generate-404-function-template": "node generate-404.js",
    "retrieve-static-files-and-update": "npm-run-all --serial retrieve-static-files update-static-files",
    "retrieve-static-files": "npm-run-all download:*",
    "download:robots": "if-env DOWNLOAD_ROBOTS=true && node download-file.js SITECORE_ORIGIN robots.txt SITECORE_PROXY_BASIC_AUTH || exit 0",
    "download:sitemap-images": "node download-file.js SITECORE_ORIGIN images.xml SITECORE_PROXY_BASIC_AUTH",
    "download:sitemap-documents": "node download-file.js SITECORE_ORIGIN documents.xml SITECORE_PROXY_BASIC_AUTH",
    "update-static-files": "npm-run-all updatehost:* ",
    "updatehost:robots": "if-env DOWNLOAD_ROBOTS=true && node update-host.js public/robots.txt || exit 0",
    "updatehost:sitemap-images": "node update-host.js public/images.xml",
    "updatehost:sitemap-documents": "node update-host.js public/documents.xml",
    "generate-sitemap": "node generate-sitemap.js",
    "storybook": "start-storybook -p 6006 -s ./public"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:59211",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "ie 9",
    "opera 12.1",
    "ios 6",
    "android 4"
  ],
  "dependencies": {
    "@culturehq/add-to-calendar": "^1.1.2",
    "@google-pay/button-react": "^2.2.1",
    "@loadable/component": "^5.14.1",
    "@paypal/react-paypal-js": "^4.1.1",
    "@stripe/react-stripe-js": "^1.3.0",
    "@stripe/stripe-js": "^1.13.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bowser": "^2.11.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "date-fns": "^2.22.1",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-serve-static-core": "^0.1.1",
    "fetch-retry": "^3.1.0",
    "formik": "^2.2.6",
    "html-to-react": "^1.4.3",
    "if-env": "^1.0.4",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.5.1",
    "loglevel": "^1.6.3",
    "mustache": "3.0.1",
    "next": "9.3.2",
    "next-compose-plugins": "^2.2.0",
    "node-fetch": "^2.6.0",
    "node-sass": "4.14.1",
    "object-fit-images": "^3.2.4",
    "p-limit": "^2.2.1",
    "photoswipe": "^4.1.3",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-add-to-calendar": "^0.1.5",
    "react-content-loader": "^5.0.2",
    "react-dom": "^16.13.1",
    "sass": "^1.32.7",
    "serialize-error": "^4.1.0",
    "sitemap": "^2.2.0",
    "stripe": "^8.137.0",
    "styled-components": "^5.2.1",
    "typescript": "^3.5.1",
    "winston": "^3.2.1",
    "yup": "0.31.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/dotenv": "^8.2.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.9.3",
    "@types/yup": "^0.29.11",
    "babel-loader": "^8.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "js2xmlparser": "^4.0.1",
    "ncp": "^2.0.0",
    "netlify-cli": "^3.4.0",
    "netlify-lambda": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "react-is": "^16.13.1",
    "react-test-renderer": "^17.0.1",
    "sass-loader": "10.1.1",
    "ts-jest": "^26.4.3",
    "xml2json": "^0.12.0"
  }
}
